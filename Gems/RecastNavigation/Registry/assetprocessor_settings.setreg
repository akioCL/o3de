                  "productAssetType": "{5275fa85-c737-4b7b-bee4-375521698f1e}"
                },
                "ScanFolder RecastNavigation/Assets": {
                    "watch": "@GEMROOT:RecastNavigation@/Assets",
                    "recursive": 1,
